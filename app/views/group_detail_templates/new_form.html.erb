<h1>
  Add a new group detail
</h1>


<hr>

<form action="/create_group_detail" method="post">

  <div class="form-group">
    <label for="user_id">
      User
    </label>

    <input type="text" id="user_id" name="user_id" class="form-control">
  </div>

  <div class="form-group">
    <label for="group_id">
      Group
    </label>

    <input type="text" id="group_id" name="group_id" class="form-control">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Create group detail
  </button>
</form>

<hr>

<a href="/group_details">
  Go back
</a>
